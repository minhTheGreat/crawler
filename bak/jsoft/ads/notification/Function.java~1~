package jsoft.ads.function;
import java.sql.*;
import jsoft.objects.*;
import jsoft.ShareControl;

public interface Function extends ShareControl {

    //Cac chuc nang cap nhat nguoi su dung
    public boolean addFunction(FunctionObject item);
    public boolean editFunction(FunctionObject item);
    public boolean delFunction(FunctionObject item);

    //Cac chuc nang lay du lieu
    public ResultSet getFunction(int id);
    public ResultSet getFunctions(FunctionObject similar, int at, byte total);

}
